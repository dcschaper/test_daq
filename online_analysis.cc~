#include "TGraph.h"
#include "TCanvas.h"

int main(int argc, char** argv)
{
  short buf[1024];
  TGraph gr[8];
  TCanvas c("online");

  for (int i=0;i<8;++i) {
    for (int j=0;j<128;++j) {
      gr[i].SetPoitn(j,0);
    }
    gr[i].SetLineColor(i);
    gr[i].Draw(i?"l":"al");
  }
  while (fread(buf,2,1024,stdin)) {
    for (int i=0;i<8;++i) {
      for (int j=0;j<128;++j) {
        gr[i].SetPoitn(j,);
      }
    }
    c.Modified(); c.Update();
  }
}
